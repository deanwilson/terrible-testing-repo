server {
  listen 80 default_server;
  auth_basic "Prometheus";
  auth_basic_user_file /etc/nginx/.htpasswd;

  location / {
    proxy_pass  http://prometheus-server:9090;
  }
  location /status {
    auth_basic off;
    proxy_pass http://prometheus-server:9090/status;
  }
  location /health {
    return 200 "Static health check";
  }
}
